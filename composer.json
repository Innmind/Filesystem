{
    "name": "innmind/filesystem",
    "type": "library",
    "description": "Filesystem abstraction layer",
    "keywords": ["filesystem"],
    "homepage": "http://github.com/Innmind/filesystem",
    "license": "MIT",
    "authors": [
        {
            "name": "Baptiste Langlade",
            "email": "langlade.baptiste@gmail.com"
        }
    ],
    "support": {
        "issues": "http://github.com/Innmind/filesystem/issues"
    },
    "require": {
        "php": "~7.4",
        "innmind/immutable": "~3.0",
        "symfony/filesystem": "^3.0|~4.0|~5.0",
        "symfony/finder": "~3.0|~4.0|~5.0",
        "innmind/stream": "~2.0",
        "innmind/media-type": "^1.1",
        "innmind/url": "^3.1"
    },
    "autoload": {
        "psr-4": {
            "Innmind\\Filesystem\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\Innmind\\Filesystem\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "~8.0",
        "vimeo/psalm": "^3.7"
    }
}
